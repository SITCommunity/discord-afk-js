{
  "name": "discord-afk-js",
  "version": "1.5.3",
  "description": "discord-afk-js package is a convenient tool for creating AFK commands without the need for a database",
  "contributors": [
    "BrokenEdtz <brokenedtz@cyrateam.xyz>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyraTeam/discord-afk-js.git"
  },
  "keywords": [
    "discord",
    "discord-afk",
    "bot",
    "bots",
    "discord-bot",
    "discord.js",
    "afks",
    "afk",
    "discord-afk-js",
    "nodatabase",
    "nodb"
  ],
  "author": "brokenedtz",
  "funding": "https://github.com/SITCommunity/discord-afk-js?sponsor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CyraTeam/discord-afk-js/issues"
  },
  "homepage": "https://cyrateam.xyz/discord-afk-js",
  "main": "./src/index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "test": "pnpm run test:typescript && pnpm run test:jest",
    "test:typescript": "tsc --noEmit && tsd",
    "test:jest": "jest --coverage",
    "update": "pnpm --recursive update --interactive",
    "update:latest": "pnpm --recursive update --interactive --latest",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./typings/index.d.mts",
        "default": "./src/index.js"
      },
      "require": {
        "types": "./typings/index.d.ts",
        "default": "./src/index.js"
      }
    }
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "src",
    "typings"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.8.0",
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "package-json": "^10.0.1",
    "semver": "^7.6.0",
    "semver-diff": "^4.0.0",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^4.1.0",
    "@types/node": "^22.7.5",
    "@types/semver": "^7.5.6",
    "chai": "^5.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "prettier": "3.3.3",
    "tsd": "^0.31.2",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "provenance": true
  }
}
